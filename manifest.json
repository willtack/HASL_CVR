{
	"name": "hasl-decode",
	"label": "HASL Decoder: Hadamard-encoded ASL (HASL) postprocessing pipeline",
	"description": "CVR Hadamard-encoded multi-delay ASL data(HASL) postprocessing pipeline.",
	"version": "0.0.5_0.0.5",
	"inputs": {
		"asl": {
			"base": "file",
			"description": "Path to raw ASL image"
		},
		"m0": {
			"base": "file",
			"description": "Path to M0 image"
		},
		"mprage": {
			"base": "file",
			"description": "Path to MPRAGE"
		}
	},
	"config": {
		"hyperCO2": {
			"default": false,
			"description": "Is there a C02 challenge in this study?",
			"type": "boolean"
		},
		"stimulus_start": {
			"description": "Timepoint when C02 stimulus starts. (hyperC02 must be set to true for this to apply.)",
			 "type": "integer"
		}
	},
	"environment": {
		"FLYWHEEL": "/flywheel/v0",
		"LANG": "C.UTF-8",
		"LD_LIBRARY_PATH": "/usr/local/MATLAB/MATLAB_Runtime/v96//runtime/glnxa64:/usr/local/MATLAB/MATLAB_Runtime/v96//bin/glnxa64:/usr/local/MATLAB/MATLAB_Runtime/v96//sys/os/glnxa64:/usr/local/MATLAB/MATLAB_Runtime/v96//sys/opengl/lib/glnxa64",
		"PATH": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
	},
	"command": "/flywheel/v0/run",
	"author": "William Tackett",
	"maintainer": "William Tackett",
	"license": "BSD-3-Clause",
	"source": "https://github.com/willtack/HASL_CVR",
	"url": "https://hub.docker.com/r/willtack/hasl/tags",
	"custom": {
		"gear-builder": {
			"category": "analysis",
			"image": "willtack/hasl:0.0.5"
		},
		"flywheel": {
			"suite": "BrainScienceCenter"
		}
	}
}
